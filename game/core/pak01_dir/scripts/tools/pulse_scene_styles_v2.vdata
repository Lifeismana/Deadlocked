				{
					Foreground_Color = "$YellowBright"
					.PortPlug = 
					{
						Background_Color = "$YellowBright"
					}
				}
				"&.FireAndForget .OutflowPort.Port:nth-child(n+2), .OutflowPort.FireAndForget" = 
				{
					Foreground_Color = "$GreenBright"
					.PortPlug = 
					{
						Background_Color = "$GreenBright"
					}
				}
				.OutflowIfElse = 
				{
					.OutflowIfElseLabel = 
					{
						Foreground_Color = "$colorBase"
					}
				}
				.OutflowPort = 
				{
					.PortPlug = 
					{
						Border_Color = "$GreyDark"
					}
					"&.PauseForPreviousEvents .PortPlug" = 
					{
						BackgroundImage = "tools/images/pulse_editor/timeline_event_wait_lg.png"
						Background_Color = "$GreyDark"
						ImageTint = "$YellowBright"
					}
				}
			}
		}
		".node_theme.$className:hover" = 
		{
			Background_Color = "$bgColorHover"
			Foreground_Color = "$colorHover"
