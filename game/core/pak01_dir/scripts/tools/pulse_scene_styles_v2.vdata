			{
				Background_Color = "$paramColorHover"
			}
			.Port.InflowPort = 
			{
				"&:nth-child(n+2) .PortPlug" = 
				{
					Background_Color = "$flowColor"
					Border_Color = "$bgColorHover"
				}
			}
		}
	}
