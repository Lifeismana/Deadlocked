	"@mixin node_theme( $className, $variables)" = 
	{
		"@include-variables" = "$variables"
		".node_theme.$className" = 
		{
			Background_Color = "$bgColorBase"
			Foreground_Color = "$colorBase"
			Border_Color = "$borderColor"
			Font = "$typeFace"
			"&.BaseValue" = 
			{
				Background_Color = "transparent"
				"&.ValueType_Bool .BaseValueInnerContainer::before" = 
				{
					Background_Color = "$data_type_bool"
				}
				"&.ValueType_Number .BaseValueInnerContainer::before" = 
				{
					Background_Color = "$data_type_number"
				}
				"&.ValueType_String .BaseValueInnerContainer::before" = 
				{
					Background_Color = "$data_type_string"
				}
				"&.ValueType_Other .BaseValueInnerContainer::before" = 
				{
					Background_Color = "$data_type_other"
				}
			}
			.BaseValueInnerContainer = 
			{
				Background_Color = "$bgColorBase"
			}
			"&.BaseValue.ValueWithoutPortLabels" = 
			{
				".NodeRows .ValueNodeHeader" = 
				{
					Background_Color = "$LightAccessoryColor"
					Border = "2px Inside $bgColorBase"
				}
				"&:hover, &:hover-descendant" = 
				{
					".NodeRows .ValueNodeHeader" = 
					{
						Background_Color = "$LightAccessoryColorHover"
						Border = "2px Inside $bgColorHover"
					}
				}
			}
			"&.BaseFlow" = 
			{
				"&.CPulseCell_Inflow_Wait" = 
				{
					".PortLabel, .HeaderText" = 
					{
						Foreground_Color = "$colorBase"
					}
					.ValueType_Number = 
					{
						.PortValueLabel = 
						{
							Foreground_Color = "$colorBase"
						}
					}
				}
				.HeaderIcon = 
				{
					ImageTint = "$iconColor"
				}
			}
			.FlowIcon = 
			{
				ImageTint = "$flowColor"
				"&::after" = 
				{
					ImageTint = "$flowColorAccessory"
				}
			}
			".FlowEntry, .FlowExit" = 
			{
				Background_Color = "$flowColor"
			}
			".NodeRow::after" = 
			{
				ImageTint = "$DashedLineColor"
			}
			.DragHandle = 
			{
				Background_Color = "$flowColor"
			}
			.DragHandleGrip = 
			{
				Background_Color = "$bgColorHover"
			}
			.drag_handle_grip = 
			{
				Background_Color = "$bgColorHover"
			}
			".flow:hover .drag_handle" = 
			{
				Background_Color = "$dragColorHover"
			}
			.icon_style_20x20 = 
			{
				ImageTint = "$flowColor"
			}
			.HeaderText = 
			{
				Font = "$typeFace"
				Foreground_Color = "$colorBase"
				"&.SubHeaderText" = 
				{
					Font = "Dinish,10,-1,5,50,0,0,0,0,0,Italic"
					Foreground_Color = "$colorBase"
				}
			}
			"&.ValueWithoutPortLabels .HeaderText" = 
			{
				Font = "Barlow,-1,36,18,40,0,0,0,0,0,Regular"
				Foreground_Color = "$colorBase"
				HAlign = "CENTER"
				VAlign = "CENTER"
			}
			.RequirementLayout_Separator_Line = 
			{
				ImageTint = "$node_base_accessory"
			}
			"&.BaseConnection" = 
			{
				"&.ValueType_Bool" = 
				{
					Foreground_Color = "$data_type_bool"
				}
				"&.ValueType_Number" = 
				{
					Foreground_Color = "$data_type_number"
				}
				"&.ValueType_String" = 
				{
					Foreground_Color = "$data_type_string"
				}
				"&.ValueType_Other" = 
				{
					Foreground_Color = "$data_type_other"
				}
			}
			.Port = 
			{
				.PortPlug = 
				{
					Border_Color = "$PortBorderColor"
					Background_Color = "$PortBackgroundColor"
				}
				.PortlabelContainer = 
				{
					.PortLabel = 
					{
						Foreground_Color = "$colorBase"
					}
				}
				"&.InflowPort" = 
				{
					.PortPlug = 
					{
						Background_Color = "$InFlowOutFlowPortBackgroundColor"
					}
					"&:nth-child(n+2)" = 
					{
						Foreground_Color = "$InFlowOutFlowPortBackgroundColorAlt"
