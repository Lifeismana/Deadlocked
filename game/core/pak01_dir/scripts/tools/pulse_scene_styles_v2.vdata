			}
		}
		".node_theme.$className:selected" = 
		{
			Background_Color = "$bgColorHover"
			Foreground_Color = "$colorHover"
			"&.BaseValue" = 
			{
				Background_Color = "transparent"
			}
			.param_group = 
			{
				Background_Color = "$paramColorHover"
			}
		}
	}
