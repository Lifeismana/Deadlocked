	"@mixin node_theme( $className, $variables)" = 
	{
		"@include-variables" = "$variables"
		".node_theme.$className" = 
		{
			Background_Color = "$bgColorBase"
			Foreground_Color = "$colorBase"
			Border_Color = "$borderColor"
			Font = "$typeFace"
			"&.BaseValue" = 
			{
				Background_Color = "transparent"
			}
			.BaseValueInnerContainer = 
			{
				Background_Color = "$bgColorBase"
			}
			.FlowIcon = 
			{
				ImageTint = "$flowColor"
				"&::after" = 
				{
					ImageTint = "$flowColorAccessory"
				}
			}
			".FlowEntry, .FlowExit" = 
			{
				Background_Color = "$flowColor"
			}
			".NodeRow::after" = 
			{
				ImageTint = "$DashedLineColor"
			}
			.DragHandle = 
			{
				Background_Color = "$flowColor"
			}
			.DragHandleGrip = 
			{
				Background_Color = "$bgColorHover"
			}
			.drag_handle_grip = 
			{
				Background_Color = "$bgColorHover"
			}
			".flow:hover .drag_handle" = 
			{
				Background_Color = "$dragColorHover"
			}
			.icon_style_20x20 = 
			{
				ImageTint = "$flowColor"
			}
			.HeaderText = 
			{
				Padding_Left = 6
				Padding_Right = 6
				Font = "$typeFace"
				"&.SubHeaderText" = 
				{
					Font = "Dinish,10,-1,5,50,0,0,0,0,0,Italic"
				}
			}
			.RequirementLayout_Separator_Line = 
			{
				ImageTint = "$node_base_accessory"
			}
			"&.BaseConnection" = 
			{
				"&.ValueType_Bool" = 
				{
					Foreground_Color = "$data_type_bool"
				}
				"&.ValueType_Number" = 
				{
					Foreground_Color = "$data_type_number"
				}
				"&.ValueType_String" = 
				{
					Foreground_Color = "$data_type_string"
				}
				"&.ValueType_Other" = 
				{
					Foreground_Color = "$data_type_other"
				}
			}
			.Port = 
			{
				.PortPlug = 
				{
					Border_Color = "$PortBorderColor"
					Background_Color = "$PortBackgroundColor"
				}
				"&.OutflowPort .PortPlug, &.InflowPort .PortPlug" = 
				{
					Background_Color = "$InFlowOutFlowPortBackgroundColor"
				}
				"&.IsValidPortDragLocation .PortPlug, &.IsDragPort .PortPlug" = 
				{
					Border_Color = "$DashedLineColor"
				}
				"&.ParamsOnLeft .Row" = 
				{
					Background_Color = "$Black"
				}
				"&.ValueType_Bool" = 
				{
					".PortValueLabel, .PortPlug" = 
					{
						Foreground_Color = "$data_type_bool"
						ImageTint = "$data_type_bool"
					}
				}
				"&.ValueType_Number" = 
				{
					".PortValueLabel, .PortPlug" = 
					{
						Foreground_Color = "$data_type_number"
						ImageTint = "$data_type_number"
					}
				}
				"&.ValueType_String" = 
				{
					".PortValueLabel, .PortPlug" = 
					{
						Foreground_Color = "$data_type_string"
						ImageTint = "$data_type_string"
					}
				}
				"&.ValueType_Other" = 
				{
					".PortValueLabel, .PortPlug" = 
					{
						Foreground_Color = "$data_type_other"
						ImageTint = "$data_type_other"
					}
				}
			}
		}
		".node_theme.$className:hover" = 
		{
			Background_Color = "$bgColorHover"
			Foreground_Color = "$colorHover"
			".value_node_ctn::after" = 
			{
				ImageTint = "$DashedLineColorHover"
			}
			".NodeRow::after" = 
			{
				ImageTint = "$DashedLineColorHover"
			}
			.param_group = 
			{
				Background_Color = "$paramColorHover"
			}
			"&.BaseValue" = 
			{
				Background_Color = "transparent"
			}
			.BaseValueInnerContainer = 
			{
				Background_Color = "$bgColorHover"
				Foreground_Color = "$colorHover"
			}
		}
		".node_theme.$className:hover-descendant" = 
		{
			Background_Color = "$bgColorHover"
			Foreground_Color = "$colorHover"
			".value_node_ctn::after" = 
			{
				ImageTint = "$DashedLineColorHover"
			}
			".NodeRow::after" = 
			{
